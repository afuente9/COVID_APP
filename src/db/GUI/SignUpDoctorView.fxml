            <TextField fx:id="ColNumTextField" layoutX="271.0" layoutY="316.0" prefHeight="26.0" prefWidth="175.0" promptText="Collegiate number" />
            <TextField fx:id="DateTextField" layoutX="271.0" layoutY="274.0" prefHeight="26.0" prefWidth="175.0" promptText="yyyy-mm-dd" />
            <Label fx:id="hoslabel" layoutX="38.0" layoutY="355.0" prefHeight="24.0" prefWidth="90.0" text="Hospital" textFill="#35657f">
               <font>
                  <Font name="Times New Roman" size="21.0" />
               </font>
            </Label>
            <TextField fx:id="HospitalTextField" layoutX="271.0" layoutY="354.0" prefHeight="26.0" prefWidth="175.0" promptText="Hospital" />
