               <font>
                  <Font name="Times New Roman" size="21.0" />
               </font>
            </Label>
            <Label fx:id="EmailLabel" layoutX="37.0" layoutY="78.0" prefHeight="27.0" prefWidth="98.0" text="User name:" textFill="#35657f">
               <font>
                  <Font name="Times New Roman" size="21.0" />
               </font>
            </Label>
            <TextField fx:id="EmaDcotorText" layoutX="271.0" layoutY="78.0" prefHeight="26.0" prefWidth="175.0" promptText="User name" />
            <Button fx:id="createUserbutton" layoutX="453.0" layoutY="450.0" mnemonicParsing="false" onAction="#oncreateuser" prefHeight="43.0" prefWidth="90.0" text="Create user" textFill="#2f5569">
               <font>
                  <Font name="Times New Roman" size="15.0" />
               </font></Button>
            <PasswordField fx:id="pasDoctorTxtF" layoutX="271.0" layoutY="114.0" prefHeight="26.0" prefWidth="175.0" promptText="Password" />
            <Label fx:id="repeatLabel" layoutX="37.0" layoutY="153.0" prefHeight="27.0" prefWidth="193.0" text="Confirm password:" textFill="#35657f">
               <font>
                  <Font name="Times New Roman" size="21.0" />
               </font>
            </Label>
            <PasswordField fx:id="RepeatPassword" layoutX="271.0" layoutY="153.0" prefHeight="26.0" prefWidth="175.0" promptText="Confirm password" />
      </children>
    </AnchorPane>
  </children>
</VBox>
